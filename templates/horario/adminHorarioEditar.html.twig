{% extends 'admin/indexAdmin.html.twig' %}

{% block title %}Editar Horario
{% endblock %}

{% block main %}
	<h3>Editar Franja Horaria: {{ franja }}</h1>
	<section id="editar-horario">
		{{ form(form) }}

		{% for message in app.flashes('error') %}
			<div class="flash-error">
				{{ message }}
			</div>
		{% endfor %}

	</section>
    <a href="{{ path('app_admin_horarios') }}">Cancelar</a>
{% endblock %}
