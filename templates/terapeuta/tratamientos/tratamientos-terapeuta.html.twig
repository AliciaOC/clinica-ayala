<table id="tratamientos_propios" class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Mis Tratamientos</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        {% for tratamiento in tratamientos %}
            <tr>
                <td>{{ tratamiento.nombre }}</td>
                <td>
                    <button class="enlace_accion quitar" data-id='{{ tratamiento.id }}'>Quitar</button>
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
