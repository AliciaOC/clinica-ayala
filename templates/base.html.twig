<!DOCTYPE html>
<html lang="es">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Clínica Ayala
			{% endblock %}
		</title>

		{% block javascripts %}
			{% block importmap %}
				{{ importmap('app') }}
			{% endblock %}
		{% endblock %}
	</head>
	<body>
		{% block body %}
			<header>
				<a class="logo" href=" {{ path('app_home') }} ">Clínica Ayala</a>	

				{% block encabezado %}
					<a id="enlace_perfil" href="{{ path(perfil) }}">{{ nombre }}</a>
				{% endblock %}
			</header>
			<main>
				{% block main %}{% endblock %}
			</main>
			<footer>
				<a class="logo" href=" {{ path('app_home') }} ">Clínica Ayala</a>	
				<ul id="contacto_footer">
						<li>Avenida Francisco Ayala s/n</li>
						<li>+34 000000000</li>
						<li>correo@correo.correo</li>
				</ul>
			</footer>
		{% endblock %}
	</body>
</html>
