{% extends 'admin/indexAdmin.html.twig' %}

{% block title %}Editar Terapeuta
{% endblock %}

{% block main %}
	<h3>Editar Terapeuta: {{ terapeuta.nombre }}</h1>
	<section id="editar-terapeuta">
		{{form_start(formEditTerapeuta) }}
        {{ form_widget(formEditTerapeuta) }}
        	<button class="btn btn-primary" type="submit">Guardar</button>
			<a class="enlace_cancelar enlace_form" href="{{ path('app_admin_terapeutas') }}">Cancelar</a>
        {{ form_end(formEditTerapeuta) }}

		{% for message in app.flashes('error') %}
			<div class="flash-error">
				{{ message }}
			</div>
		{% endfor %}
	</section>

{% endblock %}
